{
  "compilerOptions": {
    "outDir": "dist",
    "lib": ["es2020"],
    "sourceMap": true,
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "declaration": true
  },
  "include": ["typechain-types/**/*.ts"],
}
